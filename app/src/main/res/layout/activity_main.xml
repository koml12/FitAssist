<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="com.koml2.fitassist.MainActivity">

    <FrameLayout
            android:id="@+id/fragment_view_workout_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
    />

    <FrameLayout
            android:id="@+id/fragment_add_exercise_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
    />


    <FrameLayout
            android:id="@+id/fragment_edit_delete_exercise_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
    />

    <FrameLayout
            android:id="@+id/fragment_view_workout_list_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
    />

    <FrameLayout
            android:id="@+id/fragment_add_workout_container"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
    />

</android.support.constraint.ConstraintLayout>
